//System Libraries
#include <iostream> //I/O Library
using namespace std;//Namespace for iostream

//User Libraries

//Global Constants

//Function Prototypes

//Execution Begins Here!
int main(int argc, char** argv) {
   
    //Declare and Initialize Variables
    unsigned short miles = 350; //maximum miles able to travel given gallons of gas
    unsigned short gallon = 16; //amount of fuel
    
    //calculate mpg
    unsigned short mpg = miles/gallon; //fuel efficiency 
    
    //Output mpg
    cout<<"Car's fuel efficiency is "<< mpg << " mpg"<< endl;
    
    
    return 0;
}
